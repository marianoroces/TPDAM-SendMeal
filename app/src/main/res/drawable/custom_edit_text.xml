<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorBlanco"></solid>
            <corners android:radius="@dimen/radioRectanguloEditText"></corners>
            <stroke android:color="@color/colorCeleste" android:width="@dimen/strokeEditText"></stroke>
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorBlanco"></solid>
            <corners android:radius="@dimen/radioRectanguloEditText"></corners>
            <stroke android:color="@color/colorGris" android:width="@dimen/strokeEditText"></stroke>
        </shape>
    </item>

</selector>